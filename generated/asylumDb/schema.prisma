model tb_doss {
  doss_id     Int    @id @default(autoincrement())
  room_num    Int
  doss        String @unique(map: "doss") @db.VarChar(10)
  doss_status Int
  doss_type   String @db.VarChar(1)
  doss_sex    Int    @default(0)
}

model tb_draft {
  draft_id Int      @id @default(autoincrement())
