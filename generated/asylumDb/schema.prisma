generator client {
  provider = "prisma-client-js"
  output   = "../../generated/asylumDb"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL_ASYLUM")
}

model court_appeal_types {
  appeal_type_id Int    @id @default(autoincrement())
  appeal_type    String @db.VarChar(100)
}

model court_appeals {
  appeal_id         Int      @id @default(autoincrement())
  case_id           Int
  claim_id          Int
  court_accept_date DateTime @db.Date
  actual            Int      @default(1)
  filled_in         DateTime @default(now()) @db.Timestamp(0)
  filled_by         Int
  court_level       Int
  court_name        String?  @db.VarChar(250)
}

model court_claim_initiators {
  id             Int    @id @default(autoincrement())
  initiator_name String @db.VarChar(255)
}

model court_claim_members {
  id         Int      @id @default(autoincrement())
  claim_id   Int
  case_id    Int
  person_id  Int
  created_at DateTime @default(now()) @db.Timestamp(0)
  actual     Int      @default(1)
}

model court_claims {
  claim_id                  Int      @id @default(autoincrement())
  claim_serial_number       String   @db.VarChar(255)
  claim_date                DateTime @db.Date
  appealable_ms_decision    Int?
  appealable_court_decision Int?
  appealable_claim          Int?
  filled_by                 Int
  filled_in_date            DateTime @default(now()) @db.Timestamp(0)
  ms_notified_date          DateTime @db.Date
  ms_lawyer                 Int
  case_id                   Int
  court_level               Int
  actual                    Int      @default(1)
  initiator                 Int
  claim_status              Int      @default(0)
  appeal_type               Int
  status_change_date        DateTime @db.Date
}

model court_cliam_status {
  id           Int     @id @default(autoincrement())
  court_level  Int
  name         String  @db.VarChar(255)
  button_value String? @db.VarChar(255)
}

model court_decision_types {
  court_decision_type_id Int    @id @default(autoincrement())
  court_type             Int
  court_decision         String @db.VarChar(100)
}

model court_decisions {
  court_decision_id          Int      @id @default(autoincrement())
  appeal_id                  Int
  decission_type             Int
  decison_date               DateTime @db.Date
  decision_notification_date DateTime @db.Date
  input_date                 DateTime @default(now()) @db.Timestamp(0)
  input_by                   Int
  actual                     Int      @default(1)
}

model court_levels {
  court_id    Int    @id @default(autoincrement())
  court_title String @db.VarChar(100)
}

model cover_sign_status {
  sign_status_id   Int    @id @default(autoincrement())
  sign_status_name String @db.VarChar(255)
}

model files {
  id              Int      @id @default(autoincrement())
  file_name       String   @db.LongText
  uploaded_on     DateTime @default(now()) @db.Timestamp(0)
  file_type       Int?
  uploader        Int
  case_id         Int
  decision_id     Int?
  inter_id        Int?
  coi_id          Int?
  person_id       Int?
  draft_id        Int?
  claim_id        Int?
  appeal_id       Int?
  file_path       String?  @db.Text
  process_type_id Int?
  actual          Int      @default(1)
}

model old_case_decisions {
  old_decision_id     Int       @id @default(autoincrement())
  old_case_id         Int
  ms_decision         Int
  ms_decision_date    DateTime? @db.Date
  final_decision      Int?
  final_decision_date DateTime? @db.Date
}

model old_case_person {
  old_person_id      Int     @id @default(autoincrement())
  old_case_id        Int
  f_name_arm         String? @db.VarChar(50)
  l_name_arm         String? @db.VarChar(50)
  p_name_arm         String? @db.VarChar(100)
  f_name_eng         String? @db.VarChar(50)
  l_name_eng         String? @db.VarChar(50)
  p_name_eng         String? @db.VarChar(100)
  sex                Int
  b_day              String? @db.VarChar(2)
  b_month            String? @db.VarChar(2)
  b_year             String? @db.VarChar(4)
  role               Int
  citizenship_id     Int?
  card_num           String? @db.VarChar(8)
  doc_num            String? @db.VarChar(30)
  etnicity           Int?
  religion           Int?
  invalid            Int     @default(0)
  pregnant           Int     @default(0)
  seriously_ill      Int     @default(0)
  trafficking_victim Int     @default(0)
  violence_victim    Int     @default(0)
  comment            String? @db.Text
  illegal_border     Int     @default(0)
  transfer_moj       Int?    @default(0)
  deport_prescurator Int     @default(0)
  prison             Int     @default(0)
  image              String? @db.VarChar(250)
  pnum               String? @db.VarChar(10)
  doc_type           Int?
  document_num       String? @db.VarChar(20)
  status             Int?
}

model old_cases {
  old_case_id         Int      @id @default(autoincrement())
  application_date    DateTime @db.Date
  citizenship         Int
  RA_address          String?  @db.VarChar(50)
  building            String?  @db.VarChar(10)
  apartment           String?  @db.VarChar(10)
  marz_id             Int?
  community_id        Int?
  bnak_id             Int?
  unaccompanied_child Int      @default(0)
  separated_child     Int      @default(0)
  single_parent       Int      @default(0)
  prefered_language   String?  @db.VarChar(50)
  contact_tel         String?  @db.VarChar(30)
  comment             String?  @db.Text
}

model tb_arm_com {
  community_id Int    @id @default(autoincrement())
  marz_id      Int
  ADM3_PCODE   String @db.VarChar(7)
  ADM3_ARM     String @db.VarChar(30)
  ADM3_EN      String @db.VarChar(30)
  exist        Int    @default(0)
  active       Int    @default(0)
}

model tb_arm_com__ {
  community_id Int    @id @default(autoincrement())
  marz_id      Int
  ADM3_PCODE   String @db.VarChar(7)
  ADM3_ARM     String @db.VarChar(30)
  ADM3_EN      String @db.VarChar(30)
}

model tb_calendar {
  interview_id    Int       @id @default(autoincrement())
  case_id         Int
  user_id         Int
  inter_comment   String    @db.Text
  inter_date_from DateTime? @db.DateTime(0)
  inter_date_to   DateTime? @db.DateTime(0)
  text_color      String    @db.VarChar(50)
  border_color    String    @db.VarChar(50)
  actual_event    Int       @default(1)
}

model tb_cancel {
  id            Int      @id @default(autoincrement())
  personal_id   Int
  case_id       Int
  decision_date DateTime @db.Date
  uploader      Int
  uploaded      DateTime @default(now()) @db.Timestamp(0)
  file_id       Int
}

model tb_cards {
  card_id      Int      @id @default(autoincrement())
  serial       String   @db.VarChar(2)
  card_number  Int
  personal_id  Int
  issued       DateTime @db.Date
  full_address String?  @db.Text
  valid        DateTime @db.Date
  bar          String   @db.Text
