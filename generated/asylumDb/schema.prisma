model tb_coi {
  coi_id           Int       @id @default(autoincrement())
  from_officer     Int
  to_coispec       Int
  case_id          Int
  request_date     DateTime  @db.Date
  request_deadline DateTime  @db.Date
  description      String?   @db.Text
  request_text     String    @db.Text
  coi_state        Int
  request_count    Int?
  response_date    DateTime? @db.Date
  coi_status       Int
}

model tb_country {
  country_id  Int    @id @default(autoincrement())
  country_eng String @db.VarChar(150)
  country_arm String @db.VarChar(150)
}

model tb_cover_files {
  cover_file_id  Int    @id @default(autoincrement())
  type           Int
  file_name      String @db.VarChar(255)
  cover_status   Int
  case_id        Int
