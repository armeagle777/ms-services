}

model tb_case {
  case_id             Int       @id @default(autoincrement())
  application_date    DateTime  @db.Date
  input_date          DateTime  @default(now()) @db.Timestamp(0)
  reg_by              Int
  officer             Int?
  preferred_lawyer    Int       @default(0)
  unaccompanied_child Int       @default(0)
  separated_child     Int       @default(0)
  single_parent       Int       @default(0)
  prefered_language   String?   @db.VarChar(25)
  RA_marz             Int?
  RA_community        Int?
  RA_settlement       Int?
  RA_street           String?   @db.VarChar(255)
  RA_building         String?   @db.VarChar(20)
  RA_apartment        String?   @db.VarChar(20)
  contact_tel         String?   @db.VarChar(50)
  contact_email       String?   @db.VarChar(100)
  comment             String?   @db.Text
  case_status         Int?
  mul_num             String?   @db.VarChar(25)
  mul_date            DateTime? @db.Date
  MS_lawyer           Int?
  special             Int       @default(0)
  reopened            Int       @default(0)
  attached_case       Int?
}

model tb_case_status {
  case_status_id Int    @id @default(autoincrement())
  case_status    String @db.VarChar(30)
}

model tb_checkin {
  checkin_id    Int       @id @default(autoincrement())
  checkin_date  DateTime? @db.Date
