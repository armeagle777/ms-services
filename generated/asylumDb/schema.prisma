  deadline_type    String @db.VarChar(50)
}

model tb_decision_status {
  decision_status_id Int    @id @default(autoincrement())
  decision_status    String @db.VarChar(50)
}

model tb_decision_types {
  decision_type_id Int    @id @default(autoincrement())
  decision_type    String @db.VarChar(50)
}

model tb_decisions {
  decision_id      Int      @id @default(autoincrement())
  case_id          Int
  decision_type    Int
  decison_date     DateTime @default(now()) @db.Timestamp(0)
  decision_status  Int
